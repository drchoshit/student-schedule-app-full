{
  "name": "student-schedule-app-full",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "frontend": "npm run dev --prefix frontend",
    "backend": "node backend/server.js",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "start": "cross-env NODE_ENV=production node backend/server.js",
    "build": "npm --prefix frontend run build",
    "postinstall": "npm install --prefix frontend && npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "file-saver": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "solapi": "^5.5.1",
    "sqlite": "^4.2.1",
    "sqlite3": "^5.1.7",
    "xlsx": "^0.18.5"
  }
}
